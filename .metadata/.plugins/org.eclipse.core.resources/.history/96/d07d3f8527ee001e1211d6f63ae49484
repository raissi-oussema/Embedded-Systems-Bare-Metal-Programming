/* Define all needed address BusReg to toggle a led attached pin5 on port B with proteus 8*/
#include 			"stm32f401xc.h"
#include 			"adc.h"
#include            <stdio.h>
#include 			"uart.h"
uint32_t value;
void main(void)
{
	uart2_rxtx_init();

	pa1_adc_init();
	start_conversion();
	while (1){
		value = adc_read();
		uart2_write(value);
	}
}
